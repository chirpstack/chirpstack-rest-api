            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "queueItem": {
                  "type": "object",
                  "properties": {
                    "fCnt": {
                      "type": "integer",
                      "format": "int64",
                      "description": "Downlink frame-counter.\nThis will be automatically set on enqueue."
                    },
                    "fPort": {
                      "type": "integer",
                      "format": "int64",
                      "description": "FPort (must be \u003e 0)."
